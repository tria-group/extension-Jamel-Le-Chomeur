{
    "manifest_version": 3,
    "author": "TrackZone",
    "name": "Jamel Le Chomeur",
    "icons": {
        "128": "image/happy-jamel.png"
    },
    "description": "Jamel Le Chomeur - Extension Chrome",
    "homepage_url": "https://www.twitch.tv/jamel_le_chomeur",
    "version": "2.0.0",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_title": "Jamel Le Chomeur",
        "default_popup": "popup.html"
    },
    "permissions": ["background", "notifications"],
    "host_permissions": ["<all_urls>"]
}
