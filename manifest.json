{
    "manifest_version": 2,
    "name": "Jamel le chomeur",
    "icons": {
        "128": "image/logo128x128.png"
    },
    "description": "Jamel le chomeur - Extension Firefox",
    "homepage_url": "https://www.twitch.tv/jamel_le_chomeur",
    "version": "2.1.2",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Jamel le chomeur",
        "default_popup": "popup.html"
    },
    "permissions": ["background", "notifications"],
    "host_permissions": ["<all_urls>"]
}
